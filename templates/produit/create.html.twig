{% extends 'base.html.twig' %}

{% block title %}Créer un produit{% endblock %}

{% block body %}
<div class="container mt-4">
    <h1>Créer un produit</h1>
    {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.description) }}
        {{ form_row(form.prix) }}
        {{ form_row(form.creationdate) }}
        {{ form_row(form.active) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}
</div>
{% endblock %}
