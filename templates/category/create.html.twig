{% extends 'base.html.twig' %}

{% block title %}Créer une catégorie{% endblock %}

{% block body %}
<div class="container mt-4">
    <h1>Créer une catégorie</h1>
    {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.description) }}
        {{ form_row(form.save) }}
    {{ form_end(form) }}
</div>
{% endblock %}
